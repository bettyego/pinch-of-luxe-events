# Pinch of Luxe Events Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_BASE_URL=http://localhost:3001

# EmailJS Configuration
# Get these values from your EmailJS dashboard (https://www.emailjs.com/)
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_INQUIRY_TEMPLATE_ID=your_inquiry_template_id_here
VITE_EMAILJS_CONTACT_TEMPLATE_ID=your_contact_template_id_here
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Analytics (Optional)
VITE_GA_TRACKING_ID=your_google_analytics_id_here

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# App Configuration
VITE_APP_NAME="Pinch of Luxe Events"
VITE_APP_VERSION=1.0.0

# Development
VITE_DEBUG_MODE=false
