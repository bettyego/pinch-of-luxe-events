{
  "name": "pinch-of-luxe-events",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:debug": "vite build --mode development",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "fix:websocket": "node scripts/fix-websocket.js",
    "fix:websocket:disable": "node scripts/fix-websocket.js disable-hmr",
    "fix:websocket:status": "node scripts/fix-websocket.js status",

    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "echo 'TypeScript not configured - using JavaScript'",
    "audit-fix": "npm audit fix",
    "health-check": "npm run lint && npm run test:run && npm run build",
    "start": "npm run dev",
    "serve": "npm run preview",
    "validate": "npm run lint && npm run type-check && npm run test:run",
    "prepare": "echo 'No prepare script needed'",
    "dev:fresh": "npm cache clean --force && rm -rf node_modules/.vite && npm run dev",
    "clear-cache": "npm cache clean --force && rm -rf node_modules/.vite && rm -rf dist"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@tailwindcss/postcss": "^4.1.3",
    "@tailwindcss/vite": "^4.1.3",
    "aos": "^2.3.4",
    "framer-motion": "^12.6.5",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.0",
    "react-select": "^5.10.1",
    "react-select-country-list": "^2.2.3",
    "react-swipeable": "^7.0.2",
    "sweetalert2": "^11.17.2",
    "swiper": "^11.2.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
